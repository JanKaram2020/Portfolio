{
  "name": "tony-portfolio-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:uno": "unocss '*/**/*.tsx' --out-file=globals.css --watch",
    "dev": "concurrently 'yarn dev:next' 'yarn dev:uno'",
    "build": "unocss '*/**/*.tsx' --out-file=globals.css && next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "test": "vitest run",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.5",
    "framer-motion": "4",
    "js-cookie": "^3.0.5",
    "next": "^14.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.32.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "15.6.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.15",
    "@types/react-test-renderer": "^18.0.3",
    "@unocss/postcss": "^0.56.5",
    "@unocss/transformer-attributify-jsx": "^0.56.5",
    "@unocss/transformer-variant-group": "^0.56.5",
    "@unocss/webpack": "^0.56.5",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "10.1.0",
    "concurrently": "^8.2.2",
    "jsdom": "^22.1.0",
    "next-sitemap": "^1.6.148",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.4.3",
    "unocss": "^0.58.5",
    "vite": "^5.1.5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  }
}
