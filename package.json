{
  "name": "tony-portfolio-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:uno": "unocss '*/**/*.tsx' --out-file=globals.css --watch",
    "dev": "concurrently 'yarn dev:next' 'yarn dev:uno'",
    "build": "unocss '*/**/*.tsx' --out-file=globals.css && next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "test": "vitest run",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "framer-motion": "4",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.32.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "15.6.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.15",
    "@types/react-test-renderer": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "@unocss/postcss": "^0.56.5",
    "@unocss/transformer-attributify-jsx": "^0.56.5",
    "@unocss/transformer-variant-group": "^0.56.5",
    "@unocss/webpack": "^0.56.5",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "10.1.0",
    "concurrently": "^8.2.2",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-wesbos": "2.0.0-beta.7",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jsdom": "^22.1.0",
    "next-sitemap": "^1.6.148",
    "postcss": "^8.4.31",
    "prettier": "2.3.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.4.3",
    "unocss": "^0.56.5",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0"
  },
  "resolutions": {
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0"
  }
}
